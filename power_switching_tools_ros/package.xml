  <version>2.1.31</version>
  <description>ROS tools for power switching tools</description>

  <maintainer email="pazeshun3682@gmail.com">Shun Hasegawa</maintainer>
  <maintainer email="k-okada@jsk.t.u-tokyo.ac.jp">Kei Okada</maintainer>
  <author email="pazeshun3682@gmail.com">Shun Hasegawa</author>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>
