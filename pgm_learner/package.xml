<?xml version="1.0"?>
<package format="3">
  <name>pgm_learner</name>
  <version>2.1.24</version>
  <description>Parameter/Structure Estimation and Inference for Bayesian Belief Network</description>

  <maintainer email="furushchev@jsk.imi.i.u-tokyo.ac.jp">Yuki Furuta</maintainer>
  <license>MIT</license>
  <author email="furushchev@jsk.imi.i.u-tokyo.ac.jp">Yuki Furuta</author>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>rospy</build_depend>
  <build_depend>message_generation</build_depend>
  <exec_depend>python-libpgm-pip</exec_depend>
  <exec_depend>rospy</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 2">python-scipy</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 3">python3-scipy</exec_depend>
  <exec_depend>message_runtime</exec_depend>
  <test_depend>rostest</test_depend>
  <export>
  </export>
</package>
