#!/usr/bin/bash

# Original arguments
args=("$@")

# Filtered arguments (up to first "--")
filtered_args=()
for arg in "${args[@]}"; do
  if [[ "$arg" == -- ]]; then
    break
  fi
  filtered_args+=("$arg")
done

# Now use "${filtered_args[@]}" instead of "$@"
exec rosrun voicevox3 python3 $(rospack find voicevox3)/voicevox_engine/run.py ${filtered_args[@]}
