<?xml version="1.0"?>
<package format="3">
  <name>julius_ros</name>
  <version>2.1.31</version>
  <description>The julius_ros package</description>
  <maintainer email="furushchev@jsk.imi.i.u-tokyo.ac.jp">Yuki Furuta</maintainer>
  <author email="furushchev@jsk.imi.i.u-tokyo.ac.jp">Yuki Furuta</author>
  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>
  <exec_depend>audio_capture</exec_depend>
  <exec_depend>audio_common_msgs</exec_depend>
  <exec_depend>julius</exec_depend>
  <exec_depend>julius-voxforge</exec_depend>
  <exec_depend>nkf</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 2">python-lxml</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 3">python3-lxml</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 2">python-rospkg</exec_depend>
  <exec_depend condition="$ROS_PYTHON_VERSION == 3">python3-rospkg</exec_depend>
  <exec_depend>rospy</exec_depend>
  <exec_depend>sound_play</exec_depend>
  <exec_depend>sound-theme-freedesktop</exec_depend>
  <exec_depend>speech_recognition_msgs</exec_depend>
  <exec_depend>std_srvs</exec_depend>

  <test_depend>rostest</test_depend>

  <export>
  </export>
</package>
